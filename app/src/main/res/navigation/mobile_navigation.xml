<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_doctor">

    <fragment
        android:id="@+id/navigation_doctor"
        android:name="com.example.mydoctor.ui.doctor.DoctorFragment"
        android:label="@string/title_doctor"
        tools:layout="@layout/fragment_doctor" />

    <fragment
        android:id="@+id/navigation_messages"
        android:name="com.example.mydoctor.ui.messages.MessagesFragment"
        android:label="@string/title_messages"
        tools:layout="@layout/fragment_messages" />

    <fragment
        android:id="@+id/navigation_hospitals"
        android:name="com.example.mydoctor.ui.hospitals.HospitalsFragment"
        android:label="@string/title_hospitals"
        tools:layout="@layout/fragment_hospitals" />
</navigation>