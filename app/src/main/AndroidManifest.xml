<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.anantyan.exerciseproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExerciseProject.NoActionBar">
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.MessagesDetailActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.BaseFragmentActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.signup.SignUpActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.signin.SignInActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.OnBoardActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.SplashActivity"
            android:exported="false" />
        <activity
            android:name="id.anantyan.exerciseproject.ui.activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ExerciseProject.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>