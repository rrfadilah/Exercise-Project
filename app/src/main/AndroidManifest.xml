<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dzamir.exercise_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Exercise_Project.NoActionBar">
        <activity
            android:name=".ui.splashscreen.SplashActivity"
            android:exported="false" />
        <activity
            android:name=".home.Home_activity"
            android:exported="false"
            android:label="@string/title_activity_home"></activity>
        <activity
            android:name=".ui.signup.OnSignupActivity"
            android:exported="false" />
        <activity
            android:name=".galleryFragment.GalleryActivity"
            android:exported="false"></activity>
        <activity
            android:name=".samplefragment.PicturesActivity"
            android:exported="false" />
        <activity
            android:name=".ui.signin.OnSigninActivity"
            android:exported="false" />
        <activity
            android:name=".ui.onboarding.OnBoardingActivity"
            android:exported="false"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Exercise_Project.Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>