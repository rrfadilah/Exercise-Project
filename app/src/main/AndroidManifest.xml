<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mutawalli.exercise_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExerciseProject">
        <activity
            android:name="com.mutawalli.exercise_project.home.HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home" />
        <activity
            android:name="com.mutawalli.exercise_project.galleryfragment.GalleryActivity"
            android:exported="true" />
        <activity
            android:name="com.mutawalli.exercise_project.samplefragment.PicturesActivity"
            android:exported="true"></activity>
        <activity
            android:name="com.mutawalli.exercise_project.SignInActivity"
            android:exported="true"></activity>
        <activity
            android:name="com.mutawalli.exercise_project.OnBoardingActivity"
            android:exported="false" />
        <activity
            android:name="com.mutawalli.exercise_project.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>